{
  "compilerOptions": {
    "allowUnusedLabels": true,
    "alwaysStrict": true,
    "baseUrl": ".",
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "window.jsxFactory",
    "lib": [
      "dom",
      "es2015"
    ],
    "moduleResolution": "node",
    "sourceMap": true,
    "types": [
      "node"
    ]
  },
  "exclude": [
    "**/tsignore/**/*.ts",
    "**/tsignore/**/*.tsx"
  ]
}
